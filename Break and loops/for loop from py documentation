from typing import List

nums = [2,7]
target = 9

class Solution:
    
    def twoSum(nums: List[int], target: int) -> List[int]:
        for x in range(len(nums)):
            i = 0
            j = 1
            sum = nums[i]+ nums[j]
            if sum == target:
                return i, j

print(Solution.twoSum(nums, target))