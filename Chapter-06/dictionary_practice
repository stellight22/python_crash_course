print(personal_info_dict)
print(personal_info_dict['first_name'])
print(personal_info_dict['last_name'])
print(personal_info_dict['age'])
print(personal_info_dict['city'])

favorite_numbers ={ 'Abigail': 12, 'Akhil': 34, 'Jens': 13, 'Ailan': 43, 'Ahyeon': 22}
for key in favorite_numbers:
    print("The person named " + str(key) + " has a favorite number: " + str(favorite_numbers[key]))


